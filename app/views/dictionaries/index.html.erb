<h1>Listing dictionaries</h1>

<table>
  <tr>
    <th>Collection</th>
    <th>Column</th>
    <th>Conditional</th>
    <th>Keypart</th>
    <th>Alias</th>
    <th>Datatype</th>
    <th>Require</th>
    <th>Sm</th>
    <th>Default</th>
    <th>Conversion</th>
    <th>Validation</th>
    <th>Options</th>
    <th>Label</th>
    <th>Display</th>
    <th>Characteristics</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dictionaries.each do |dictionary| %>
  <tr>
    <td><%= dictionary.collection %></td>
    <td><%= dictionary.column %></td>
    <td><%= dictionary.conditional %></td>
    <td><%= dictionary.keypart %></td>
    <td><%= dictionary.alias %></td>
    <td><%= dictionary.datatype %></td>
    <td><%= dictionary.require %></td>
    <td><%= dictionary.sm %></td>
    <td><%= dictionary.default %></td>
    <td><%= dictionary.conversion %></td>
    <td><%= dictionary.validation %></td>
    <td><%= dictionary.options %></td>
    <td><%= dictionary.label %></td>
    <td><%= dictionary.display %></td>
    <td><%= dictionary.characteristics %></td>
    <td><%= link_to 'Show', dictionary %></td>
    <td><%= link_to 'Edit', edit_dictionary_path(dictionary) %></td>
    <td><%= link_to 'Destroy', dictionary, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dictionary', new_dictionary_path %>
